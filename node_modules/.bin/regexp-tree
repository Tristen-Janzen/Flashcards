#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../babel-preset-react-app/node_modules/regexp-tree/bin/regexp-tree" "$@"
  ret=$?
else 
  node  "$basedir/../babel-preset-react-app/node_modules/regexp-tree/bin/regexp-tree" "$@"
  ret=$?
fi
exit $ret
